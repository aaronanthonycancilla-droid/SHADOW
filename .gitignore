<html lang="en">
<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>Digital Clock</title>
  <style>
    body {
      font-family: Arial, sans-serif;
      text-align: center;
      margin: 0;
      padding: 0;
      background-color: #f5f5f5;
    }

    h1 {
      margin: 20px 0;
    }

    .clocks {
      display: flex;
      flex-direction: column;
      align-items: center;
      gap: 15px;
    }

    .clock {
      padding: 10px 20px;
      border: 1px solid #ccc;
      border-radius: 8px;
      background: #ffffff;
      box-shadow: 0 2px 5px rgba(0, 0, 0, 0.1);
      font-size: 1.5em;
    }
  </style>
</head>
<body>
  <h1>Digital Clock - World Time Zones</h1>
  <div class="clocks">
    <div class="clock" id="utc"></div>
    <div class="clock" id="new-york"></div>
    <div class="clock" id="london"></div>
    <div class="clock" id="tokyo"></div>
  </div>

  <script>
    function updateClock() {
      const timeZones = {
        utc: "UTC",
        "new-york": "America/New_York",
        london: "Europe/London",
        tokyo: "Asia/Tokyo",
      };

      for (const [id, timeZone] of Object.entries(timeZones)) {
        const clockElement = document.getElementById(id);
        const now = new Date();
        const options = { weekday: 'short', year: 'numeric', month: 'short', day: 'numeric', hour: '2-digit', minute: '2-digit', second: '2-digit', timeZone };
        clockElement.textContent = new Intl.DateTimeFormat("en-US", options).format(now);
      }
    }

    // Update the clock every second
    setInterval(updateClock, 1000);
    // Run updateClock immediately
    updateClock();
  </script>
</body>
